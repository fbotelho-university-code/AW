		<resource path="noticias.php"> 
			<method name="GET"> 
				<request>
					<param name="start" style="query" type="xsd:int"/>
					<doc xml:lang="en" title="Start value.">
						Indicates the 'id' where the response should start.
						0 is used if not defined.
						Must be greater than 0.
					</doc>
					<param name="count" style="query" type="xsd:int"/>
					<doc xml:lang="en" title="How many elements to be returned.">
						Indicates how many elements should be returned.
						Must be greater than 0.
						10 is used if not defined.
						If not so many elements are available, all are returned.
					</doc>
					<param name="Accept" style="header" type="xsd:string"/>
					<doc xml:lang="en" title="Response format.">
						Can be 'text/xml' (default) or 'json'.
					</doc>
				</request>
				<response status="200"> 
					<representation id="Noticias" mediaType="text/xml" element="noticias:noticias"/> 
				</response>
			</method>
			<method name="POST"> 
				<request>
					<representation id="Noticia" mediaType="text/xml" element="noticia:noticia"/> 
				</request>
				</response status="201">
				</response>
			</method> 
