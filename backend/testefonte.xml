<xml>
	<uri>
		<base>http://pesquisa.sapo.pt/</base>
		<query>
			<variable>
				<id>barra</id>
				<value>noticias</value>
			</variable>
			<variable>
				<id>cluster</id>
				<value>0</value>
			</variable>
			<variable>
				<id>format</id>
				<value>rss</value>
			</variable>
			<variable>
				<id>location</id>
				<value>pt</value>
			</variable>
			<variable>
				<id>st</id>
				<value>local</value>
			</variable>
			<variable>
				<id>limit</id>
				<value>10</value>
			</variable>
		</query>
		<search>
			<id>q</id>
		</search>
	</uri>
	<noticia>
		<data_pub>pubDate</data_pub>
		<assunto>title</assunto>
		<descricao>description</descricao>
		<url>link</url>
	</noticia>
</xml>